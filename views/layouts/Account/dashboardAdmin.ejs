<!-- Dashboard Navigation Section -->
<div class="container p-xl-4 mb-4">
  <div class="row">
    <div class="col-12">
      <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container-fluid">
          <button
            class="navbar-toggler"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#navbarNav"
            aria-controls="navbarNav"
            aria-expanded="false"
            aria-label="Toggle navigation"
          ></button>
          <div class="navbar-collapse" id="navbarNav" data-aos="fade-down">
            <ul
              class="dashboardSection navbar-nav d-flex justify-content-center align-items-center w-100 gap-3"
            >
              <li class="nav-item">
                <a
                  class="nav-link dashboardPage active"
                  aria-current="page"
                  href="#"
                  id="productSection"
                  >Product</a
                >
              </li>
              <li class="nav-item">
                <a class="nav-link dashboardPage" href="#" id="partnerSection"
                  >Partner</a
                >
              </li>
            </ul>
          </div>
        </div>
      </nav>
    </div>
  </div>
</div>

<!-- Product Table Section -->
<div class="container dashboardTableProduct pb-5" data-aos="fade-up">
  <div class="row">
    <div class="col-12">
      <div class="card">
        <div class="card-header">
          <div class="my-3">
            <div class="sidebar-box search-form-wrap">
              <form class="search-form">
                <div class="form-group">
                  <i class="fa fa-search icon" aria-hidden="true"></i>
                  <input
                    type="text"
                    class="form-control productSearchBar"
                    id="s"
                    placeholder="Search products..."
                  />
                </div>
              </form>
            </div>
          </div>

          <div class="d-flex justify-content-center mb-3">
            <button class="btn btn-success addProductBtn">Add Product</button>
          </div>
        </div>

        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>No</th>
                  <th>Name</th>
                  <th>Image</th>
                  <th>Rating</th>
                  <th>Price</th>
                  <th>Actions</th>
                </tr>
              </thead>
              <tbody id="product-table">
                <!-- Product Table Row -->
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- Partner Table Section -->
<div class="container dashboardTablePartner pb-5">
  <div class="row">
    <div class="col-12">
      <div class="card">
        <div class="card-header">
          <div class="my-3">
            <div class="sidebar-box search-form-wrap">
              <form class="search-form">
                <div class="form-group">
                  <i class="fa fa-search icon" aria-hidden="true"></i>
                  <input
                    type="text"
                    class="form-control partnerSearchBar"
                    id="s"
                    placeholder="Search partner..."
                  />
                </div>
              </form>
            </div>
          </div>

          <div class="d-flex justify-content-center mb-3">
            <button class="btn btn-success addPartnerBtn">Add Partner</button>
          </div>
        </div>

        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>No</th>
                  <th>Name</th>
                  <th>Logo</th>
                  <th>Website</th>
                  <th>Actions</th>
                </tr>
              </thead>
              <tbody id="partner-table">
                <!-- Partner Table Row -->
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
